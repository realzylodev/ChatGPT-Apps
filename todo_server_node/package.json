{
  "name": "todo-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for todo management with ChatGPT integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "clean": "rm -rf dist",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:unit": "vitest --run tests/unit",
    "test:coverage": "vitest --run --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "zod": "^3.22.4",
    "uuid": "^9.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "keywords": ["mcp", "todo", "chatgpt", "server"],
  "author": "",
  "license": "MIT"
}